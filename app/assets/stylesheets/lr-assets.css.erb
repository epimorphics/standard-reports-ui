@font-face {
  font-family: 'GeoHeadlineRegular';
  src: url(<%= asset_path 'geoheadline-webfont.eot' %>);
  src: url(<%= asset_path 'geoheadline-webfont.eot' %>#iefix ) format('embedded-opentype'),
       url(<%= asset_path 'geoheadline-webfont.woff' %>) format('woff'),
       url(<%= asset_path 'geoheadline-webfont.ttf' %>) format('truetype'),
       url(<%= asset_path 'geoheadline-webfont.svg#GeoHeadlineRegular' %>) format('svg');
  font-weight: normal;
  font-style: normal;
}

.map-button {
  background-image: url( <%= asset_path 'ew-icon.png' %>);
  background-position: center center;
  background-repeat: no-repeat;
  height: 35px;
  width: 35px;
}
